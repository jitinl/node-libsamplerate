{
    "name": "node-libsamplerate",
    "version": "1.0.0",
    "description": "Native bindings for libsamplerate",
    "main": "index.js",
    "license": "MIT",
    "keywords": [
        "libsamplerate",
        "samplerate",
        "audio",
        "sample",
        "rate",
        "convert",
        "resample",
        "downsample",
        "upsample",
        "n-api",
        "node-addon-api",
        "cmake-js",
        "S16_LE",
        "S32_LE",
        "pcm",
        "wav"
    ],
    "scripts": {
        "install": "cmake-js compile",
        "prebuild": "prebuild -t 1 --runtime napi --backend cmake-js  --all --strip --verbose",
        "upload": "prebuild  -t 1 --runtime napi --backend cmake-js --upload ${GITHUB_TOKEN}",
        "test": "nyc mocha test/*.test.js",
        "build:dev": "cmake-js build --debug",
        "build": "cmake-js build",
        "rebuild:dev": "cmake-js rebuild --debug",
        "rebuild": "cmake-js rebuild",
        "clean": "node-gyp clean"
    },
    "dependencies": {
        "bindings": "^1.3.0",
        "cmake-js": "^4.0.0",
        "node-addon-api": "^1.4.0",
        "prebuild-install": "^6.0.1"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "repository": "github:jitinl/node-libsamplerate",
    "bugs": {
        "url": "https://github.com/jitinl/node-libsamplerate/issues"
    },
    "homepage": "https://github.com/jitinl/node-libsamplerate#readme",
    "devDependencies": {
        "audio-generator": "^2.1.2",
        "mocha": "^5.2.0",
        "nyc": "^13.0.1",
        "sinon": "^6.3.3"
    },
    "nyc": {
        "extension": [
            ".js"
        ],
        "exclude": [
            "example.js",
            "waveheader.js",
            "test/*.js"
        ]
    },
    "binary": {
        "napi_versions": [
            1
        ]
    }
}
